{
  "kind": "singleType",
  "collectionName": "meta_datas",
  "info": {
    "singularName": "meta-data",
    "pluralName": "meta-datas",
    "displayName": "Meta Data",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "homepageTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "default": "Ride to Walk - Therapeutic Horseback Riding in Penryn, CA",
      "minLength": 40
    },
    "subsequentPageTitleTemplate": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "default": "%s | Ride to Walk - Therapeutic Horseback Riding in Penryn, CA"
    },
    "homepageDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "default": "Ride to Walk, located in Penryn, CA, is a non-profit organization providing therapeutic horseback riding programs for individuals with disabilities.",
      "required": true
    },
    "openGraphDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "default": "Empowering individuals with disabilities through therapeutic horseback riding and other equine-assisted activities."
    },
    "openGraphImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
